$h1:80px;
$h1-mobile:32px;
$h1-line-height-mobile:48px;
$h2:64px;
$h2-line-height:72px;
$h3-mobile: 24px;
$h3: 48px;
$h4: 32px;
$h5:20px;
$h5-mobile:18px;
$h6:16px;
h1, .h1{
  font-size: $h1-mobile;
  line-height: $h1-line-height-mobile;
  @media #{$small-break} {
    font-size: $h1;
    line-height: $h1;
  }
}
h2, .h2{
  font-size: $h4;
  line-height: $h1-mobile;
  @media #{$small-break} {
    font-size: $h2;
    line-height: $h2-line-height;
  }
}
h3, .h3{
  font-size: $h3-mobile;
  @media #{$small-break} {
    font-size: $h3;
  }
}
h4, .h4{
  font-size: $h5;
  line-height: $h4;
  @media #{$small-break} {
    font-size: $h4;
    line-height: $h1-mobile;
  }
}
h5,.h5{
  font-size: $h5-mobile;
  line-height: $h4;
  @media #{$small-break} {
    font-size: $h5;
    line-height: $h4;
  }
}
h6, .h6{
  font-size: $h6;
  line-height: $h3-mobile;
}
.xsmall-font{
  font-size:14px;
}
.sub-heading{
  font-size: $h1-mobile;
  line-height: $h1-line-height-mobile;
  @media #{$small-break} {
    font-size: 56px;
    line-height: $h2;
  }
}
